<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hanaonestock.member.model.dao.MemberMapper">
    <select id="getAllMember" parameterType="com.hanaonestock.member.model.dto.Member" resultType="com.hanaonestock.member.model.dto.Member">
        SELECT
        *
        FROM member
    </select>
    <insert id="updateOrSaveUser" parameterType="com.hanaonestock.member.model.dto.Member">
        INSERT INTO member(id,email,name) VALUES (seq_member.NEXTVAL,#{email},#{name})
    </insert>
</mapper>